[Unit]
Description=selenium node
After=syslog.target network.target

[Service]
ExecStart=/usr/bin/xvfb-run /usr/bin/java -Dwebdriver.chrome.driver="D:\IBETAF\drivers\chromedriver.exe" -jar {{ selenium_install_dir }}/selenium/selenium-server-standalone-3.8.1.jar -role node -port 5552 -hub http://localhost:4444/grid/register -browser "browserName=chrome,platform=WINDOWS,maxInstances=5"
Restart=on-failure
RestartSec=20s

[Install]
WantedBy=multi-user.target
