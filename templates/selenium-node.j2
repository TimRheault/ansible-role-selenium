[Unit]
Description=selenium node
After=syslog.target network.target

[Service]
ExecStart=/usr/bin/java -Dwebdriver.chrome.driver=/usr/bin/chromedriver -jar {{ selenium_install_dir }}/selenium/selenium-server-standalone-3.8.1.jar -role node -port 5552 -hub http://localhost:4444/grid/register -browser 'browserName=chrome,platform=LINUX,maxInstances=5' 2>>/cwt/log/selenium/selenium_ChromeNode.log
Restart=on-failure
RestartSec=20s

[Install]
WantedBy=multi-user.target
