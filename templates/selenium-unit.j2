[Unit]
Description=selenium test framework
After=syslog.target network.target

[Service]
User=mwcomm
ExecStart=/usr/bin/java -jar {{ selenium_install_dir }}/selenium/selenium-server-standalone-3.8.1.jar -role hub -log /cwt/log/selenium/selenium_hub.log
Restart=on-failure
RestartSec=20s

[Install]
WantedBy=multi-user.target
